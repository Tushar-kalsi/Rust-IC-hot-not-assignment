{
  "canisters": {
    "todo_ic_backend": {
      "candid": "src/todo_ic_backend/todo_ic_backend.did",
      "package": "todo_ic_backend",
      "type": "rust"
    },
    "todo_ic_frontend": {
      "dependencies": [
        "todo_ic_backend"
      ],
      "source": [
        "src/todo_ic_frontend/dist"
      ],
      "type": "assets",
      "workspace": "todo_ic_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "playground": {
      "providers": ["https://icp0.io"],
      "type": "persistent"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  },
  "output_env_file": ".env",
  "version": 1
}